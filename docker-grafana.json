{
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "ryaneleary/grafana:0.1.1",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 80, "hostPort": 0, "servicePort": 0, "protocol": "tcp" }
      ]
    }
  },
  "id": "/stats/grafana",
  "instances": 1,
  "cpus": 0.25,
  "mem": 256,
  "ports": [80],
  "uris": [],
  "env": {
    "INFLUXDB_SRV": "_influxdb._tcp.marathon.mesos",
    "INFLUXDB_NAME": "site_dev",
    "INFLUXDB_USER": "site_dev",
    "INFLUXDB_PASS": "site_dev",
    "HTTP_PASS": "grafana",
    "INFLUXDB_IS_GRAFANADB": "true"
  }
}
